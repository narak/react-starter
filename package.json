{
    "name": "naraks-react-starter",
    "version": "0.0.1",
    "author": "karansheth@gmail.com",
    "description": "This is boiler plate for basic react apps. It includes the config for babel, webpack, css-modules, etc.",
    "license": "MIT",
    "dependencies": {
        "@babel/core": "^7.8.4",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.0.6",
        "classnames": "^2.2.6",
        "css-loader": "^3.4.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-react": "^7.18.3",
        "file-loader": "^5.1.0",
        "prettier": "^1.19.1",
        "raw-loader": "^4.0.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-hot-loader": "^4.12.20",
        "style-loader": "^1.1.3",
